estimate_elevation_dem <- function(rds_path, tif_dir) {
spdf <- get_spdf(rds_path)
dem_file <- list.files(tif_dir, pattern=".tif", full.names = T)
DEM <- raster(dem_file)
DEM.gps <- projectRaster(DEM, crs = crs(spdf))
return(DEM.gps)
}
