
## Violin plot for rate vs cows ##

# Install and load required packages

install.packages("ggplot2")
library(ggplot2)

# Read .rds file and make it a single dataframe

data <- readRDS("cow_data.rds")
data <- dplyr::bind_rows(data, id = NULL)

# violin plot with ggplot

library(dplyr)
violin_plot <- ggplot(data %>% filter(Rate < 3, Cow!="9959"), aes(x = Cow, y = Rate))+
  geom_violin()
violin_plot + theme(axis.text.x = element_text(angle = 45, hjust = 1))


